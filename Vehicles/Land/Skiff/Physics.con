rem *** Skiff_Floater ***
ObjectTemplate.create FloatingBundle Skiff_Floater
ObjectTemplate.setHullHeight 1000
ObjectTemplate.setFloatMaxLift 9.81
ObjectTemplate.setFloatMinLift 9.81
ObjectTemplate.setSinkingSpeedMod 0
ObjectTemplate.setAutomaticReset 1

rem *** Skiff_Wheel ***
ObjectTemplate.create Spring Skiff_Wheel
ObjectTemplate.geometry corsair_Bwheel_m1
ObjectTemplate.hasMobilePhysics 1
ObjectTemplate.hasCollisionPhysics 1
ObjectTemplate.hasResponsePhysics 1
ObjectTemplate.Grip c_PGFDummyGrip
ObjectTemplate.setStrength 24
ObjectTemplate.setDamping 12
ObjectTemplate.createInvisible 1


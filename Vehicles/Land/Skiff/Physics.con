rem *** Skiff_Engine ***
ObjectTemplate.create Engine Skiff_Engine
ObjectTemplate.setNetworkableInfo SkiffEngineInfo
ObjectTemplate.loadSoundScript Sounds/SkiffEngine.ssc
ObjectTemplate.setMinRotation 0/0/-100
ObjectTemplate.setMaxRotation 0/0/100
ObjectTemplate.setMaxSpeed 0/0/7200
ObjectTemplate.setAcceleration 0/0/2700
ObjectTemplate.setInputToRoll c_PIThrottle
ObjectTemplate.setAutomaticReset 1
ObjectTemplate.setEngineType c_ETPlane
ObjectTemplate.setTorque 76
ObjectTemplate.setDifferential 7

rem *** Skiff_Vertical_Engine ***
ObjectTemplate.create Engine Skiff_Vertical_Engine
ObjectTemplate.setNetworkableInfo SkiffEngineInfo
ObjectTemplate.setMinRotation 0/0/-100
ObjectTemplate.setMaxRotation 0/0/100
ObjectTemplate.setMaxSpeed 0/0/1000
ObjectTemplate.setAcceleration 0/0/-1000
ObjectTemplate.setInputToRoll c_PIPitch
ObjectTemplate.setAutomaticReset 1
ObjectTemplate.setEngineType c_ETPlane
